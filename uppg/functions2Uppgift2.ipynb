   "source": [
    "import numpy as np\n",
    "def meantrig(H):\n",
    "    sum=0\n",
    "    for i in H:\n",
    "        sum=sum+np.sin(i)**2\n",
    "    return(sum/len(H))\n",
    "\n",
    "L=list(np.arange(0,2*np.pi,0.001))\n",
    "print(meantrig(L))"
   ]
