   "source": [
    "L=input(\"Ange ett ord så undersöker programmet om det är en palindrom\")\n",
    "\n",
    "M=list(L)\n",
    "\n",
    "l=len(M)\n",
    "s=0\n",
    "for i in range(int(l/2.)):\n",
    "    if M[i]==M[l-1-i]: s=s+1\n",
    "\n",
    "if s==int(l/2.):\n",
    "    print(\"Palindrom\")\n",
    "else:\n",
    "    print(\"Ej Palindrom\")\n"
   ]
