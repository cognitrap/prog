   "source": [
    "import numpy as np\n",
    "n=1; max=5\n",
    "A=np.array([[2,-1],[-3,0]])\n",
    "An=np.array([[1.,0.],[0.,1.]])\n",
    "\n",
    "for n in np.arange(1,max+1):\n",
    "    An=An@A\n",
    "    EV=np.linalg.eig(An)\n",
    "    print(\"Egenvärdena för n={} är \".format(n), EV[0])\n"
   ]
  },
